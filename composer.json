{
    "name": "bnb-computer-services/your-plugin-name",
    "description": "Wordpress plugin to integrate Alfaomega eBooks with WooCommerce",
    "type": "wordpress-plugin",
    "require": {
        "automattic/woocommerce": "^3.1",
        "nesbot/carbon": "^2.72",
        "vlucas/phpdotenv": "^5.6",
        "htmlburger/carbon-fields": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "AlfaomegaEbooks\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "livan2r/carbon-field-choices": "dev-develop"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
